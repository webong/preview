{
    "name": "preview/preview",
    "description": "A laravel wrapper on pre and phpx",
    "keywords": [
        "pre",
        "phpx",
        "view",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "illuminate/view": "^7.12",
        "opis/closure": "^3.1",
        "nikic/include-interceptor": "^0.1.1",
        "pre/phpx-html": "^0.3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "5.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Preview\\": "src"
        },
        "files": [
            "src/autoload.php",
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Preview\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Preview\\PreviewServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
